{
  "table_name": "revenue_starting_balance",
  "primary_key": ["date", "value_type_id"],
  "fields": {
    "date": {
      "type": "DATE NOT NULL",
      "description": "Foreign key to dates(date)"
    },
    "value_type_id": {
      "type": "INTEGER NOT NULL",
      "description": "Foreign key to value_types(id)"
    },
    "nr_of_customers": {
      "type": "INTEGER NOT NULL",
      "description": "Number of customers related to the revenue change"
    },
    "mrr_per_customer": {
      "type": "DOUBLE NOT NULL",
      "description": "Value per customer related to the revenue change"
    },
    "total_mrr": {
      "type": "DOUBLE",
      "description": "Total Monthly Recurring Revenue as defined as volume (nr_of_customers) x price (value_per_customer)"
    }
  },
  "foreign_keys": [
    {
      "field": "date",
      "references": "dates(date)"
    },
    {
      "field": "value_type_id",
      "references": "value_types(id)"
    }
  ]
}
